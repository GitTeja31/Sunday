node(''){
stage('Initilize'){
 def mavenhome = tool 'maven'
 env.PATH = "${mavenhome}/bin:${env.PATH}"
}
stage('clone'){
git branch: 'master',
credentialsId: '44ce4a49-2ecb-47c5-8f3f-2a9002fa0a13',
url: 'https://github.com/babu479/Sunday.git'
}
stage('Build'){
sh '''
cd Java-project
mvn clean install'''
}
stage('Deploy'){
build job: 'Deploy'
}
}
